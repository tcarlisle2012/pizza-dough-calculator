{
  "expo": {
    "name": "DoughCalculator",
    "slug": "DoughCalculator",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/dough-calculator-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pizza-dough.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/dough-calculator-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tcarlisle.DoughCalculator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 34,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d13cdc2a-8dc3-4860-90fe-aa20c013d196"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d13cdc2a-8dc3-4860-90fe-aa20c013d196"
    }
  }
